
{
	"masterPicture": "public/icon.png",
	"iconsPath": "/",
	"design": {
		"ios": {
			"pictureAspect": "backgroundAndMargin",
			"backgroundColor": "#ffffff",
			"margin": "14%",
			"assets": {
				"ios6AndPriorIcons": true,
				"ios7AndLaterIcons": true,
				"precomposedIcons": true,
				"declareOnlyDefaultIcon": true
			},
			"appName": "abschaffen.jetzt"
		},
		"desktopBrowser": {
			"design": "raw"
		},
		"windows": {
			"pictureAspect": "noChange",
			"backgroundColor": "#ffffff",
			"onConflict": "override",
			"assets": {
				"windows80Ie10Tile": true,
				"windows10Ie11EdgeTiles": {
					"small": true,
					"medium": true,
					"big": true,
					"rectangle": true
				}
			},
			"appName": "abschaffen.jetzt"
		},
		"androidChrome": {
			"pictureAspect": "backgroundAndMargin",
			"margin": "17%",
			"backgroundColor": "#ffffff",
			"themeColor": "#ffffff",
			"manifest": {
				"name": "abschaffen.jetzt",
				"startUrl": "https://abschaffen.jetzt",
				"display": "standalone",
				"orientation": "notSet",
				"onConflict": "override",
				"declared": true
			},
			"assets": {
				"legacyIcon": false,
				"lowResolutionIcons": true
			}
		},
		"safariPinnedTab": {
			"pictureAspect": "silhouette",
			"themeColor": "#000000"
		}
	},
	"settings": {
		"scalingAlgorithm": "Mitchell",
		"errorOnImageTooSmall": false,
		"readmeFile": false,
		"htmlCodeFile": false,
		"usePathAsIs": false
	}
}